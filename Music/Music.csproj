<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Library</OutputType>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <RootNamespace>Music</RootNamespace>
        <Title>Music Library for Console.Beep</Title>
        <Authors>Swerik</Authors>
        <Company>Swerik</Company>
        <PackageId>Music</PackageId>
        <Version>1.0.0</Version>
        <Description>Play Music over Console.Beep (which can run over you Motherboard if it supports it)</Description>
        <PackageProjectUrl>https://github.com/TheSwerik/MusicLibrary</PackageProjectUrl>
        <RepositoryUrl>https://github.com/TheSwerik/MusicLibrary</RepositoryUrl>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
<!--        <PackageIcon>icon.png</PackageIcon>-->
    </PropertyGroup>

    <ItemGroup>
<!--        <None Include="..\icon.png" Pack="true" Visible="false" PackagePath=""/>-->
    </ItemGroup>

</Project>
